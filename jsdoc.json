{
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/docdash"
    },
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "./src"
        ],
        "exclusivePattern": "(node_modules|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}